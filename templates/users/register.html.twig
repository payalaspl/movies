{% extends 'base.html.twig' %}
{% block content %}
    <h1>{{ 'user.register'|trans }}</h1>
    {% for message in app.flashes('msg') %}
	    <div class="flash-notice">
	        {{ message }}
	    </div>
	{% endfor %}
	{{ form_start(form) }}
		{{ form_row(form.username) }}
		{{ form_row(form.email) }}
	    {{ form_row(form.password.first) }}
	    {{ form_row(form.password.second) }}
	    {{ form_row(form.image) }}
	    {{ form_row(form.country) }}
	    {{ form_row(form.state) }}
   		<button class="btn btn-primary" type="submit">{{ 'btn.submit'|trans }}</button>
	{{ form_end(form) }}
	{% block javascripts %}
      {{ parent() }}
      {{ encore_entry_script_tags('state') }}
	{% endblock %}
    {% block page_contents %}{% endblock %}
{% endblock %}


